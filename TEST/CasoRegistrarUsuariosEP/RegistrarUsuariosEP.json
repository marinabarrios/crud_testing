			"name": "Crear Usuario",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const response = pm.response.json();\r",
							"\r",
							"// Save access variable to collection if it exists\r",
							"if (response.user && response.user.id) {\r",
							"    pm.collectionVariables.set(\"userId\", response.user.id);\r",
							"}\r",
							"\r",
							"// Save access variable to collection if it exists\r",
							"if (response.access) {\r",
							"    pm.collectionVariables.set(\"access\", response.access);\r",
							"}\r",
							"\r",
							"// --- Status code ---\r",
							"pm.test(\"Status code is 200, 201 o 204\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 204]);\r",
							"});\r",
							"\r",
							"pm.test(\"User has required fields\", function () {\r",
							"    const user = response.user;\r",
							"    pm.expect(user).to.have.property(\"id\");\r",
							"    pm.expect(user.id).to.be.a(\"number\");\r",
							"\r",
							"    pm.expect(user).to.have.property(\"username\").that.is.a(\"string\");\r",
							"    pm.expect(user).to.have.property(\"email\").that.is.a(\"string\");\r",
							"    pm.expect(user).to.have.property(\"first_name\").that.is.a(\"string\");\r",
							"    pm.expect(user).to.have.property(\"last_name\").that.is.a(\"string\");\r",
							"    pm.expect(user).to.have.property(\"profile\").that.is.an(\"object\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
